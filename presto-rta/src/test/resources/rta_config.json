{
  "config": {
  },
  "types": {
    "aresdb": {
      "config": {
        "aresdb.service-url": "localhost:5436",
        "fetch-timeout": "15s",
        "max-limit-without-aggregates": 1000,
        "single-split-limit": "1d",
        "cache-duration": "2d",
        "unsafe-to-cache-interval": "36h",
        "max-cache-size": "100MB"
      }
    },
    "pinot": {
      "config": {
        "controller-urls": "localhost:5436",
        "scan-parallelism-enabled": false,
        "allow-multiple-aggregations": true,
        "pinot.http-client.min-threads": 1,
        "pinot.http-client.max-threads": 4,
        "controller-rest-service": "streaming-pinot-controller-prod",
        "rest-proxy-service-for-query": "upinot-rest-proxy-prod2"
      }
    }
  }
}
